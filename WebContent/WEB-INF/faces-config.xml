<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
 <managed-bean>
  <managed-bean-name>principalMB</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.PrincipalMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>rutadinamica</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>listadoGruposMB</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.ListadoGruposMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>equipos</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>grupos</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>listaEquipos</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>listaGrupos</property-name>
   <property-class>java.util.List</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sesiones</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.session.ControlSession</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ingreso</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.Ingreso</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>partidosMB</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.PartidosMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>estadiosMB</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.EstadiosMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>equiposMB</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.EquiposMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>gruposMB</managed-bean-name>
  <managed-bean-class>com.fifa.ubosque.worldcup.beans.GruposMB</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*.*</from-view-id>
  <navigation-case>
   <from-outcome>iniciarSesion</from-outcome>
   <to-view-id>/principal/index.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>listaEquipos</from-outcome>
   <to-view-id>/principal/equipos.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>*.*</from-view-id>
  <navigation-case>
   <from-outcome>adminSesion</from-outcome>
   <to-view-id>/admin/AdminGrupos.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
